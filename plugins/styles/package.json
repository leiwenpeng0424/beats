    "version": "0.0.0",
    "main": "./npm/index.cjs",
    "module": "./npm/index.mjs",
    "types": "./npm/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./npm/index.mjs",
